@charset "UTF-8";
#top{
	width: 100%;
	height: 100px;
	background-color: silver;
}
#top li{
	float: left;
	padding-right: 20px;
	padding-top: 10px;
}
#left{
	float: left;
	width: 20%;
	height: 500px;
	background-color: orange;
}
#content{
	float: right;
	width: 79%;
	height: 500px;
	overflow: auto;
}
#footer{
	float: left;
	width: 100%;
	height: 100px;
	background-color: maroon;
}
.error{
	color: red;
}




